    {
        "id": "cd9af9d6e32cb54c",
        "type": "function",
        "z": "66389bf566fd4d78",
        "name": "function 1",
        "func": "var payload = msg.payload;\n\nvar parts = payload.split(\", \");\n\nvar temp = parseFloat(parts[0]);\nvar hum = parseInt(parts[1]);\n\nmsg.measurement = {\n    timestamp: new Date().toISOString().slice(0, 19).replace('T', ' '),\n    temperature: temp,\n    humidity: hum\n};\n\nmsg.topic = `INSERT INTO Measurement (SensorID, date_time, temperature, humidity) VALUES (1, '${msg.measurement.timestamp}', ${msg.measurement.temperature}, ${msg.measurement.humidity});`;\n\nmsg.payload = {\n    temperature: temp,\n    humidity: hum\n};\n\nreturn msg;",
